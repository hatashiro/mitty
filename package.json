{
  "name": "mitty",
  "version": "0.1.0",
  "description": "A Brain*uck to WASM compiler",
  "homepage": "https://github.com/utatti/mitty",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utatti/mitty.git"
  },
  "keywords": [
    "brainfuck",
    "wasm"
  ],
  "main": "index.js",
  "module": "main.mjs",
  "scripts": {
    "format": "prettier --write '**/*.{json,js,mjs}'",
    "format:check": "prettier --check '**/*.{json,js,mjs}'",
    "test:interpreter": "node test interpreter",
    "test": "npm run format:check && npm run test:interpreter",
    "build:wabt": "git submodule update --init && cd wabt && git submodule update --init && make gcc-release",
    "postinstall": "npm run build:wabt"
  },
  "dependencies": {
    "esm": "^3.2.22"
  },
  "devDependencies": {
    "prettier": "^1.17.0"
  },
  "license": "MIT"
}

{
  "name": "@utatti/mitty",
  "version": "0.1.1",
  "description": "A Brain*uck to WASM compiler",
  "homepage": "https://github.com/utatti/mitty",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utatti/mitty.git"
  },
  "keywords": [
    "brainfuck",
    "wasm"
  ],
  "main": "index.js",
  "module": "main.mjs",
  "bin": {
    "mitty": "index.js"
  },
  "scripts": {
    "format": "prettier --write '**/*.{json,js,mjs}'",
    "format:check": "prettier --check '**/*.{json,js,mjs}'",
    "test:interpreter": "node test interpreter",
    "test:compiler": "node test compiler",
    "test": "npm run format:check && npm run test:interpreter && npm run test:compiler",
    "wabt": "./wabt.sh",
    "postinstall": "npm run wabt",
    "release": "npm publish --access public"
  },
  "dependencies": {
    "esm": "^3.2.22",
    "s-exify": "^0.3.1",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "prettier": "^1.17.0"
  },
  "license": "MIT"
}
